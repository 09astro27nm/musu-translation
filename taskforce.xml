<?xml version="1.0" encoding="Shift_JIS" ?>
<!--基本設定------------------------------------------------------------------>
<title>MusuMaker 1.3.3</title>
<datafolder>musume</datafolder>
<!--表示する情報-->
<information cache="0" fps="0" cpu="0" />
<!--シーンモードの設定-------------------------------------------------------->
<scenemode x="150" y="79" step="8" max="9">
    <thumbnailfolder>thumbnail</thumbnailfolder>
    <extension>tga</extension>
    <empty>thumbnail\empty.tga</empty>
</scenemode>
<!--CGモードの設定------------------------------------------------------------>
<cgmode x="120" y="79" step="8" max="9">
    <thumbnailfolder>thumbnail</thumbnailfolder>
    <folder>event</folder>
    <extension>tga</extension>
    <empty>thumbnail\empty.tga</empty>
</cgmode>
<advsystem>
    <!--画面モード-->
    <screen width="800" height="600" bpp="32" window="1" />
    <!--エフェクト-->
    <effect speed="2" fps="30" />
    <tips file="system\tips.png"  />
    <emergency file="system\emergency.png" />
    <system>
        <background file="system\system.png" />
        <quit file="system\quit.png" x="370" y="455" />
        <nodata file="system\nodata.png" />
        <thumbnail folder="thumb\" />
        <cg folder="cg\" />
    </system>
    <!--テキスト-->
    <text>
        <!--テキストウィンドウ-->
        <window file="system\window\window.png" x="9" y="362" alpha="255" />
        <!--カーソルプロンプト-->
        <nextprompt>system\prompt\prompt.png</nextprompt>
        <!--未読、既読-->
        <isread skip="1" fast="0" alpha="0" />
        <!--ノーマルモード-->
        <normalmode>
            <!--フォント-->
            <font type="0" size="26" color="#000000" shadow="#999999" hmargin="4" />
            <!-- テキストの位置(ウィンドウから)-->
            <text x="32" y="88" width="680" height="360" />
            <!-- ネームプレートの位置-->
            <name size="22" x="96" y="42" color="#000000" shadow="#999999" />
            <!--選択肢-------------------------------------------------------->
            <select x="0" y="-64" textoffsetx="16" textoffsety="-8" mode="1" effect="0" file="system\choice\bar.png" size="32" color="#000000" shadow="#666666" />
        </normalmode>
        <!--ノベルモード-->
        <novelmode>
            <text x="32" y="32" width="580" height="480" />
        </novelmode>
    </text>
    <!--バックログ-->
    <backlog>
        <window file="system\backlog\backlog.png" x="16" y="0" />
        <text size="20" x="48" y="48" width="700" height="500" />
        <slider size="16" x="720" y="32" width="8" height="512" />
        <voice file="system\backlog\voicesw.png" />
    </backlog>
</advsystem>
